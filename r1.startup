#!/bin/bash

# Activer le routage IP
sysctl -w net.ipv4.ip_forward=1

# Configurer les interfaces réseau
ip address add 172.16.5.254/24 dev eth0
ip link set dev eth0 up

# NAT pour l'accès Internet (vérifiez que eth1 est l'interface correcte)
iptables -t nat -A POSTROUTING -o eth1 -j MASQUERADE

# Configurer le serveur DNS
echo "nameserver 8.8.8.8" > /etc/resolv.conf

# Limiter le débit à 100 Mbps et garantir une latence <10ms
# Vérifiez que l'interface eth1 est correcte
tc qdisc add dev eth1 root tbf rate 100mbit burst 32kbit latency 10ms || echo "Limitation du débit échouée"

# Mettre à jour les paquets et installer dnsmasq
apt-get update && apt-get install -y dnsmasq || echo "Installation de dnsmasq échouée"

# Créer le répertoire dnsmasq.d si nécessaire
